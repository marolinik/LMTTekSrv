{
  "name": "lmtek-configurator-server",
  "version": "1.1.0",
  "description": "Backend API for LM TEK Server Configurator",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
